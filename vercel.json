{
    "version": 2,
    "buildCommand": "npm install && npm run build",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "framework": null,
    "outputDirectory": "dist",
    "crons": [
        {
            "path": "/api/cron/sync-gmail",
            "schedule": "0 */6 * * *"
        }
    ],
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        }
    ],
    "functions": {
        "api/**/*.js": {
            "maxDuration": 10
        }
    }
}